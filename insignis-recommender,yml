spring:
  application:
    name: insignis-client
  data:
    mongodb:
      database: insignis-db
      port: 27017
      uri: mongodb://localhost
server:
  port: 8091
  
#Metrics related configurations
management:
  endpoint:
    metrics:
      enabled: true
    prometheus:
      enabled: true
  endpoints:
    web:
      exposure:
        include: "*"
  metrics:
    export:
      prometheus:
        enabled: true